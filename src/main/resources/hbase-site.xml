<configuration>

    <property>
        <name>hbase.zookeeper.property.clientPort</name>
        <value>2181</value>
    </property>

    <property>
        <name>hbase.zookeeper.quorum</name>
        <!--
for Dev test on Windows, please add following ip/host config  in :

   C:\Windows\System32\drivers\etc\hosts

120.26.215.198   aliyun-tagsys  iZ23j8cgmctZ


 -->
        <!--<value>aliyun-hdmp</value>-->
        <value>songchaoqun-hbase</value>
    </property>

    <property>
        <name>hbase.zookeeper.useMulti</name>
        <value>false</value>
    </property>

    <property>
        <name>zookeeper.session.timeout</name>
        <value>90000</value>
    </property>

    <property>
        <name>zookeeper.znode.parent</name>
        <value>/hbase</value>
    </property>


</configuration>
