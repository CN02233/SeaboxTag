package com.evergrande.hdmp.usertags.action.sms;


import java.util.Map;

/**
 * @author Changhua, Wu
 *         Created on: 2/17/16,5:15 PM
 */
public class RespMsgSmsSend extends SmsResp {


    public static RespMsgSmsSend   createFromResponseMap(Map<String, Object> responseMap) {
        RespMsgSmsSend respMsgSmsSend = new RespMsgSmsSend();

        String ret_code = (String)responseMap.get("return_code");

        respMsgSmsSend.setRowResult( ret_code );
        respMsgSmsSend.setSuccess( isSuccessReturnStr( ret_code ) );  //返回结果

        return respMsgSmsSend;
    }


}
