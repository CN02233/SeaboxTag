package com.evergrande.hdmp.usertags.action.sms;

import com.evergrande.sms.entity.Report;
import org.slf4j.Logger;
import org.slf4j.LoggerFactory;

import java.util.ArrayList;
import java.util.List;
import java.util.Map;

/**
 * @author Changhua, Wu
 *         Created on: 2/17/16,5:15 PM
 */
public class RespMsgSmsRecv extends SmsResp {

    private static final Logger logger = LoggerFactory.getLogger(RespMsgSmsRecv.class);

    private String description;

    private List<Report> resultList;



    public static RespMsgSmsRecv   createFromResponseMap(Map<String, Object> responseMap) {
        RespMsgSmsRecv respMsgSmsRecv = new RespMsgSmsRecv();

        String ret_code = (String)responseMap.get("return_code");
        respMsgSmsRecv.setRowResult( ret_code );
        respMsgSmsRecv.setSuccess( isSuccessReturnStr( ret_code ) );  //返回结果
        Object descObj = responseMap.get("description");
        respMsgSmsRecv.description = (descObj != null) ? descObj.toString() : NULL_STR_FROM_API ; //返回结果描述

        try {
            List<Report> reportList = (List<Report>) responseMap.get("reportList");//回复列表
            respMsgSmsRecv.setResultList(reportList);
        } catch (Throwable e) {
            logger.error("SMS API changed, bad API catching.", e);
        }

        return respMsgSmsRecv;
    }


    public String getDescription() {
        return description;
    }

    public void setDescription(String description) {
        this.description = description;
    }

    public List<Report> getResultList() {
        return resultList;
    }

    public void setResultList(List<Report> resultList) {
        this.resultList = resultList;
    }
}
