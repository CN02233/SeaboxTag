@CHARSET "UTF-8";

/*此文件根据精准营销系统页面UI需求做相应相应调整,by SongChaoqun 2016-01-13*/
.pageit_div {
	float: right;
}

.pageit_div>span.active {
	background: #8CC4F0;
	color: #fff;
}

/*
    remove td definition here to avoid td style been overwritten,
    all table styles had defined in evergrande-table.css
*/
.pageIndex {
	background: #DEE4E9;
	color: black;
	padding: 4px 10px;
	margin: 3px;
	font-family: "微软雅黑";
	cursor: pointer;
	float: left;
	font-size: 12px;
}

.currentPage {
	background: #DEE4E9;
	color: black;
	padding: 4px 10px;
	margin: 3px;
	font-family: "微软雅黑";
	cursor: pointer;
	float: left;
	font-size: 12px;
}

.pageIndex:hover {
	background: #F45E7E;
	color: #fff;
}

.currentPage:hover {
	background: #F45E7E;
	color: #fff;
}

.pagination>li>a, .pagination>li>span {
	color: black;
	font-size:12px;
	padding: 4px 10px;
}

a:focus {outline:none}