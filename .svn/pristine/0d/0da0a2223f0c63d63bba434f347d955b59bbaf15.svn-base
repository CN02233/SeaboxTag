.modal_div{
	border-radius:0px;
}

.modal_div>div{
	border-radius:0px;
}

.modal_div>div>div{
	border-radius:0px;
	width:400px;
	height:300px;
	box-shadow: #666 0px 5px 10px 5px;
}

.modal_div_header{
    background-color:#CCCCCC;
    border-radius:0px;
    width:100%;
    height:30px;
    text-align:center;
    line-height:25px;
    font-size:18px;
    font-weight:bold;
    margin-top:0px;
    position: relative;
}
.modal_div_header .close {
	background-position:-140px -26px;
    height: 20px;
    width: 20px;
    line-height: 20px;
    position: absolute;
    right: 10px;
    top: 5px;
    text-align: center;
    opacity: 1;
}

.modal_div_content{
	width:100%;
	height:270px;
	text-align:center;
}

.modal_div_content_text{
	color:#333333;
	height:190px;
	line-height:190px;
	width:100%;
	float:left;
	font-size:18px;
}

.modal_div_content_buttons{
	height:40px;
	width:100%;
	float:left;
}

.modal_mul_content{
	line-height:30px;
	padding-top:49px;
	text-align:center;
}