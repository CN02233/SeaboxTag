.input_lable{
	background: url(/HDMP/images/searchImg.png) no-repeat; /*放大镜图标*/
	width: 15px;/*lable宽度*/
	height: 15px; /*lable高度*/
	cursor: pointer;/*鼠标放上去时变成手的形状*/
	position: absolute;/*相对于父元素绝对定位*/
	margin-top:32px;/*距离上层元素顶部32像素*/
	margin-left:22%;/*距离上层元素左部22%像素*/
	top: 0;
}

.oprtDiv{
	position:absolute;
	width:80px;
	height:160px;
	z-index:100;
	background:white;
	box-shadow: #666 0px 0px 10px;
	
}
.oprtDiv>div{
	height:40px;
	line-height:40px;
	text-align:center;
}

.oprtDiv>div:hover{
	background-color:#D6EEFF;
}

.oprtImg{
	background: url(/HDMP/images/sprite_label.png) no-repeat; /*放大镜图标*/
	background-position: -10px 0;
	width: 24px;/*lable宽度*/
	height: 24px; /*lable高度*/
	cursor: pointer;/*鼠标放上去时变成手的形状*/
	position: absolute;/*相对于父元素绝对定位*/
	border:1px solid red;
}

/*列表table中 操作td样式*/
.table-par-td{
	text-align:center;
	padding-top:200px;
	padding-bottom:0px;
}