.secondmenu li.active {
	text-decoration: none;
	background-color: #2b313a;
	/* border-left: 4px solid #ffcc33; */
}

.secondmenu li.active:before {
	position: absolute;
	content: "";
	font-size: 0;
	width: 4px;
	height: 100%;
	background: #ffcc33;
}

.secondmenu li.active a {
	color: white;
}

