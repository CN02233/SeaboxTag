.detail-title-foot{
	margin-top:1px;
	margin-right:3%;
	float:right;
	text-align:right;
	width:25%;
	height:100px;
}

.check-info-list-ul{
	margin:0;
	padding:0;
	list-style:none;
	margin-top:10px;
	margin-bottom:10px;
}

.check-info-list-ul-li{
	margin-left:5%;
	margin-right:5%;
	width:90%;
	margin-top:5px;
	margin-bottom:5px;
	border:0px solid red;
}

.tag_grp_tab{
	width:97%;
	margin-top:2%;
	margin-left:3%;
	float:left;
}

.tag_grp_tab tr{
	width:95%;
	height:100%;
	margin-left:5%;
}

.tag_grp_tab tr td:nth-child(1){
	width:90%;
	height:100%;
	border:0px;
	text-align:left; 
}

.tag_grp_tab tr td:nth-child(2){
	width:10%;
	border:0px solid #D9D9D9;
	text-align:center;
}

.tag_grp_info{
	width:100%;
	height:100%;
	background:#F7F7F7;
	border-radius:10px;
	float:left;
	border:1px solid #D9D9D9; 
}

.flowBtn{
	width:9%;
	height:100%;
	color:white;
}

.selectCss{
  /*Chrome和Firefox里面的边框是不一样的，所以复写了一下*/

  /*很关键：将默认的select选择框样式清除*/
  appearance:none;
  -moz-appearance:none;
  -webkit-appearance:none;


  /*为下拉小箭头留出一点位置，避免被文字覆盖*/
  padding-right: 14px;  
  
  background-color:#F7F7F7;
  font-color:red;
   
}

.input_lable{
	background: url(/HDMP/images/searchImg.png) no-repeat; /*放大镜图标*/
	width: 15px;/*lable宽度*/
	height: 15px; /*lable高度*/
	cursor: pointer;/*鼠标放上去时变成手的形状*/
	position: absolute;/*相对于父元素绝对定位*/
	margin-top:32px;/*距离上层元素顶部32像素*/
	margin-left:22%;/*距离上层元素左部22%像素*/
	top: 0;
}

.msgBtn{
	width:14%;
	margin-left:1%;
	background-color: #F7F7F7;
	cursor:pointer;
	height:33px;
	float:left;
	border:1px solid #ccc;
	border-radius:5px;
	text-align:center;
	-webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
          box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
  	-webkit-transition: border-color ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;
       -o-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
          transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
}

.msgBtn>div{
	width:18px;
	height:18px;
	background:url(/HDMP/images/sprite_label.png) no-repeat;
	background-position:-46px -27px;
	border:0px solid red;
	margin-left:24%;
	margin-top:25%;
}

.msg_body{
	width:100%;
	height:80%;
	float:left;
	border:0px solid black;
}

.msg_tmp{
	width:100%;
	height:30%;
	border:0px solid blue;
	text-align:left;
	float:left;
}

.msg_tmp>button{
	background-color:#6CABF6;
	color:white;
	margin:1% 3% auto auto;
	border:1px solid #6CABF6;
}	
	
.msg_params{
	width:100%;
	height:40%;
	border:0px solid red;
	text-align:left;
	float:left
}

.msg_conent{
	width:100%;
	height:30%;
	border:0px solid red;
	margin-bottom:10px;
	text-align:left;float:left
}

.msg_conent>textarea{
	margin:1% 3% auto 3%;
	padding:5px 10px 0 10px;
	border-radius:4px;
	width:94%;
	height:18%;
	border:1px solid #D1D1D1;
	float:left;
	resize:none
}

.msg_test{
	width:100%;
	height:10%;
	border:0px solid red;
	margin-bottom:10px;
	text-align:left;float:left
}

.msg_test>textarea{
	margin:1% 3% auto 3%;
	padding:5px 10px 0 10px;
	border-radius:4px;
	width:94%;
	height:18%;
	border:1px solid #D1D1D1;
	float:left;
	resize:none
}

.msg_params>table{
	margin:auto 3% auto 3%;
	width:94%;
	float:left;
}

.msg_params>table>tr{
	width:100%;
	margin:auto 3% auto 3%;
}

.msg_params_td{
	border:0;
	padding-top:10px;
}

.msg_params_td>input{
	width:95%;
	text-align:center;
}

.msg_public{
	margin-top:4%;
}

.msg_public>div{
	margin:auto 3% auto 3%;
	height:6%;
	width:94%;
	border:0px solid orange;
	text-align:left;
	float:left;
	font-size:16px;
	font-weight:bold;
}

.btn_grp{
	border:0px solid red;
	width:100%;
	height:40px;
	margin-top:20px;
	float:left;
	text-align:center;
}

.btn_grp>button{
	width:150px;
	height:100%;
	color:white;
	border:1px solid #6CABF6;
	font-weight:bold;
}

.template_modal{
	border:1px solid blue;
	margin:0 auto 0 auto;
	position:fixed;
	width:100%;
	height:100%;
}

.remove_label{
	margin-left:10px;
	cursor: pointer;
}