package com.evergrande.hdmp.usertags.action.sms;

import org.junit.Assert;
import org.junit.Test;
import org.slf4j.Logger;
import org.slf4j.LoggerFactory;

/**
 * @author Changhua, Wu
 *         Created on: 2/19/16,11:01 AM
 */
public class TemplateParaTest {

    private static final Logger logger = LoggerFactory.getLogger(TemplateParaTest.class);

    @Test
    public void testReplace() {
        TemplatePara templatePara = TemplatePara.USER_NAME;

        String result = templatePara.replaceValue("wet3  #username#  sdfefe", "1231");
        logger.debug( result );
        Assert.assertEquals( result, "wet3  1231  sdfefe");


    }

}
