

/*
###############for Campaign ############################################################
 */


/* 活动 ID */
.td_camp_id {
    width: 90px;
}

/* 活动名称 */
.td_camp_name {
    width: 140px;
}

/* 开始时间 & 结束时间  */
.td_camp_date_and_time {
    width: 150px;
}

.td_camp_date_and_time>label {
	margin-left:40px;
	margin-top:4.5px;
	border:0px solid red;
	float:left;
}

.td_camp_date_and_time>span {
	border:0px solid red;
	margin-top:5px;
	margin-left:2px;
	float:left;
}


/* 人群描述 */
.td_camp_condition_desc {
    width: 220px;
}

/* 渠道  */
.td_camp_action_way {
    width:80px;
}

/* 人群数量 */
.td_camp_user_count {
    width:100px;
}

/* 状态 */
.td_camp_status {
    width:100px;
}

/* 创建人 */
.td_camp_creator{
    width:100px;
}

/* 操作 */
.td_camp_operation {
    width:70px;
}

