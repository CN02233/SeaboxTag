
.detail-title-foot{
	margin-top:1px;
	margin-right:3%;
	float:right;
	text-align:right;
	width:25%;
	height:100px;
}

.detail-info{
	margin-left:3%;
	margin-right:3%;
	margin-top:10px;
	background-color:#eff3f6;
	width:94%;
	height:60px;
	float:left;
	line-height:60px;
}
.detail-info>ul>li{
	width:25%;
	float:left;
	list-style-type:none;
}

.indv-info{
	margin-left:5%;
	margin-right:5%;
	float:left;
	width:90%;
}

.indv-info>span{
	font-weight: bold;
	display:block;
	border-bottom:1px solid #E0E0E0;
}

.send-info{/*发送情况table*/
	width:100%;
	border: 0px solid #ccc;
	float:left;
	margin-top:10px;
}

.send-info-td{/*发送情况table中td样式*/
	text-align:left;
	border:0;
}

.send-info-val{
	font-size:20px;
	font-weight:bold;
}
.label-info-div{/*标签情况DIV样式*/
	border:0px solid black;
	margin-left:5%;
	margin-right:5%;
	float:left;
	width:90%;
}

.check-info{
	border:0px solid red;
	height:100%;
	width:38%;
	float:left
}

.check-info-list{
	width:100%;
	height:90%;
	border:1px solid #E0E0E0;
	border-radius: 10px; 
	overflow:auto;
}

.check-info-list-ul{
	margin:0;
	padding:0;
	list-style:none;
}

.check-info-list-ul-li{
	margin-left:5%;
	margin-right:5%;
	width:90%;
}

.operation_body{
	border:0px solid black;
	margin-left:3%;
	margin-right:3%;
	float:left;width:94%;
}

.operation_body>div{
	width:50%;
	height:100%;
	border:0px solid red;
}

.backBtn{
	background-position:-131px -2px;
	margin-top:10px;
	border:0px solid red;
	float:left;
}

.backFnt{
	margin-top:15px;
	margin-left:30px;
	width:35px;
	border:0px solid red;
	cursor: pointer;
}

.flowBtn{
	width:20%;
	height:100%;
	color:white;
}

.hit_info{
	border:0px solid black;
	margin-left:5%;
	margin-right:5%;
	margin-top:10px;
	float:left;width:90%;
}

.scroll-x{
	width:100%;
	height:90%;
	border:1px solid #E0E0E0;
	border-radius: 10px;
	overflow:auto;
}

.helper-msg{
	width:90%;
	height:90%;
	margin-left:5%;
	margin-right:5%;
	margin-top:15%;
	line-height:40px;
	font-size:15px;
	text-align:center;
}
